{
    "name": "avoutic/web-framework-sentry",
    "description": "Sentry module for web-framework",
    "type": "library",
    "homepage": "https://web-framework.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Avoutic",
            "email": "avoutic@gmail.com"
        }
    ],
    "require": {
        "sentry/sentry": "^3.21",
        "avoutic/web-framework": "^10",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "WebFramework\\Sentry\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
